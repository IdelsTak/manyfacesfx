<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" styleClass="advanced-fonts-root" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="-1.5" styleClass="advanced-fonts-content" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="35.0" AnchorPane.rightAnchor="104.0" AnchorPane.topAnchor="18.0">
         <children>
            <JFXToggleButton styleClass="masking-toggle" text="Enable font list masking" />
            <JFXToggleButton layoutX="10.0" layoutY="10.0" styleClass="glyphs-toggle" text="Enable Unicode glyphs and DOMRect fingerprinting protection" wrapText="true" />
            <Label maxWidth="1.7976931348623157E308" styleClass="field-header-label" text="Browser profile fonts list:">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Label>
            <VBox fx:id="fontsBox" styleClass="fonts-list-area" />
            <Hyperlink fx:id="moreFontsHyperlink" maxWidth="1.7976931348623157E308" text="...and 151 more" />
            <JFXButton fx:id="editButton" text="Edit">
               <VBox.margin>
                  <Insets top="19.0" />
               </VBox.margin>
            </JFXButton>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../styles/app.css" />
      <URL value="@../styles/profile-advanced-fonts.css" />
   </stylesheets>
</AnchorPane>
